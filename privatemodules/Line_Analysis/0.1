#%Module1.0#####################################################################
proc ModulesHelp { } {
Line Analysis tools 0.1

The bin directory is added to PATH.
}

set version "0.1"
set APPhome $::env(HOME)/Line_Analysis

module-whatis "Name: Line Analysis tools"
module-whatis "Version: 0.1"
module-whatis "Category: application, utility"
module-whatis "Keywords: Utility"
module-whatis "Description: Variety of tools for SN analysis"
module-whatis "Email contact: bwmulligan@astro.as.utexas.edu"

prepend-path PATH               $APPhome/bin
prepend-path LINE_ANALYSIS_DATA_PATH $APPhome/share
prepend-path MSDB_PATH                /home/astrobit
prepend-path PATH		$APPhome/FitViz/bin
prepend-path PATH		$APPhome/MSDBmgr/bin

